package dorkbots.dorkbots_vector2D{	import flash.display.Sprite;	//	public class CircleSprite extends Sprite 	{		private var _radius:Number;		private var _color:uint;		//		public function CircleSprite(radius:Number, aX:Number = 0, aY:Number = 0) 		{			_radius = radius;			x = aX;			y = aY;		}		//		public function drawCircle(color:uint = 0x000000, lineThickness:Number = 0, fillColor:Boolean = true):void 		{			_color = color;			graphics.lineStyle(lineThickness, _color);			if (fillColor) graphics.beginFill(color);			graphics.drawCircle(0, 0, _radius);		}		//		public function moveToVector(aV:Vector2D):void		{			x = aV.x;			y = aV.y;		}		//		public function get radius():Number 		{			return _radius;		}		//		public function get position():Vector2D 		{			return new Vector2D(x, y);		}	}}
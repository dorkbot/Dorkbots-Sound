package dorkbots_sound_example{	import flash.display.DisplayObjectContainer;		import dorkbots_sound_example.app.controller.StartupCommand;	import dorkbots_sound_example.app.notifications.Notifications;	import dorkbots_sound_example.app.notifications.notification_objects.StartupNotification;		import org.puremvc.as3.interfaces.IFacade;	import org.puremvc.as3.patterns.facade.Facade;	import org.puremvc.as3.patterns.observer.Notification;
		public class DorkbotsSoundSystemFacade extends Facade implements IFacade	{		public static const NAME:String = "DorkbotsSoundSystemFacade";				public static function getInstance():DorkbotsSoundSystemFacade		{			//trace("[" + NAME + "]");						if (instance == null)			{				instance = new DorkbotsSoundSystemFacade();			}						return instance as DorkbotsSoundSystemFacade;		}				override protected function initializeController():void		{			super.initializeController();						registerCommand( Notifications.STARTUP, StartupCommand );		}				public function startup(stage:DisplayObjectContainer):void		{			// mainClass is for testing, don't pass this param for fiinal build			var startupVO:StartupNotification = new StartupNotification(stage);			sendNotification( Notifications.STARTUP, startupVO );		}				override public function sendNotification(notificationName:String, body:Object = null, type:String = null):void		{			//trace("[" + NAME + "] sendNotification -> notificationName = " + notificationName );						notifyObservers( new Notification( notificationName, body, type ) );		}	}}